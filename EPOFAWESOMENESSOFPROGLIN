#! /usr/bin/octave -qf

% m = num de linhas
% n = num de colunas

function simplex(A,b,c,m,n,print)
	%make b positive, adjust A accordingly.
	for i = 1:n
		if b(i) < 0
			b(i) = - b(1)
			A(i,:) = -A(i,:)
		endif
	endfor

	%build the phase1 tableau
	cext = [zeros(1,n),ones(1,m)]
	tableau = [-sum(b),cext;b,A,eye(m)]
    

%	[ind x]
endfunction

m = 2
n = 3
A = [1, 2, 3; 4, 4, 5]
b = [10; 10]
c = [4; 3; 2]
simplex(A, b, c, m, n, true)
